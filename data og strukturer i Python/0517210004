# for loops
# range(start, stop, steps)

for i in range(10):
    print(i)

# ulige tal
for i in range(1, 11, 2):
    print(i)

# lige tal
for i in range(2, 11, 2):
    print(i)


# while loops
start = True

# while-loopet starter kun hvis start er True
while start:
    print("Velkommen til while-loopet")
    break # break af loopet
    
    
