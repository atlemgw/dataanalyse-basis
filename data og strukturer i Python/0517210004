# for loops
# range(start, stop, steps)

for i in range(10):
    print(i)

# ulige tal
for i in range(1, 11, 2):
    print(i)

# lige tal
for i in range(2, 11, 2):
    print(i)

for i in range(10, 0, -1):
    print(i, end = ', ')


# while loops
start = True

print() # luft

# while-loopet starter kun hvis start er True
while start:
    print("Velkommen til while-loopet")
    break # break af loopet


# euklids algoritmer udregner h√∏jeste delelige tal
# https://da.wikipedia.org/wiki/Euklids_algoritme
a, b = 1071, 462
while b:
    a, b = b, a % b
print(a)
